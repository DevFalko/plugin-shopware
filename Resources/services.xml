<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="VerignAiPhilosSearch\Subscriber\Autoloader" id="verign_ai_philos_search.subscriber.autoloader">
            <tag name="shopware.event_subscriber"/>
        </service>
        <service class="Aiphilos\Api\Items\Client" id="aiphilos.api.items.client">
        </service>
        <service class="Aiphilos\Api\Semantics\Client" id="aiphilos.api.semantics.client">
        </service>
        <service class="VerignAiPhilosSearch\Bundle\AiPhilosSearchBundle\LocaleStringMapper" id="verign_ai_philos_search_bundle_ai_philos_search.locale_string_mapper">
        </service>
        <service class="VerignAiPhilosSearch\Bundle\AiPhilosSearchBundle\Repositories\BasicArticleRepository" id="verign_ai_philos_search_bundle_ai_philos_search.repositories.article_repository">
            <argument id="db" type="service"/>
        </service>
        <service class="VerignAiPhilosSearch\Bundle\AiPhilosSearchBundle\Schemes\BasicArticleScheme" id="verign_ai_philos_search_bundle_ai_philos_search.schemes.article_scheme">
            <argument id="verign_ai_philos_search_bundle_ai_philos_search.repositories.article_repository"/>
        </service>
    </services>
</container>